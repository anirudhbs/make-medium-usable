{
  "name": "MMRA",
  "version": "0.0.1",
  "description": "A plugin to make Medium readable again!",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "js": ["index.js"],
      "matches": ["https://medium.com/*", "https://hackernoon.com/*"]
    }
  ],
  "web_accessible_resources": ["styles.css"]
}
