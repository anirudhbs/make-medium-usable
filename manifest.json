{
  "name": "#MMRA",
  "version": "0.0.1",
  "description": "A plugin to make Medium readable again!",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "permissions": ["https://*/*"],
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": ["https://*/*"]
    }
  ]
}
